#include <stdio.h>
#include <string.h>
#include <stdint.h>
#include "stm32f4xx.h"
#include "uart2.h"

#define GPIOAEN (1U << 0)
#define PIN5    (1U << 5)
#define SR_RXNE     (1U << 5)

#define LED     PIN5


int main(void)
{
	RCC->AHB1ENR |= GPIOAEN;
	GPIOA->MODER |= (1U << 10);
	GPIOA->MODER &= ~(1U << 11);
	UART2_Init();


	while(1)
	{
		printf("data");
		if(strcmp(rx_buff, "LED_ON") == 0)
		{
			GPIOA->ODR |= LED;
		}
		else if(strcmp(rx_buff, "LED_OFF") == 0)
		{
			GPIOA->ODR &= ~(LED);
		}
	}
}



